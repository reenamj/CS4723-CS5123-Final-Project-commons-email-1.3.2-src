pipeline{
  agent any
  
  stages{
    stage("build"){
      steps{
        mvn -B -DskipTests clean package
        echo 'Build stage - mye631'
      }
    }
    
    stage("test"){
      steps{
        mvn test
        echo 'Test stage - mye631'
      }
    }
    
    stage("deploy"){
      steps{
        echo 'Deliver stage - mye631'
      }
    }
  }
}
